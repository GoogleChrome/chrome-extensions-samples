{
	"name": "World Clock",
	"version": "0.1.3",
	"manifest_version": 2,
	"minimum_chrome_version": "23",
	"description": "A world clock application.",
	"icons": {
		"16": "assets/icon.png",
		"128": "assets/icon.png"
	},
	"app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
	"offline_enabled": true,
	"permissions": ["geolocation",
									"storage",
									"notifications",
									"<all_urls>"
								 ]
}