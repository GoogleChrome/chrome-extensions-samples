{
  "name": "CatBlock",
  "description": "I can't has cheezburger!",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://i.chzbgr.com/*",
    "https://*.cheezburger.com/*"
