{
  "manifest_version": 3,
  "name": "History Override",
  "description": "Overrides the History Page",
  "version": "1.0",
  "chrome_url_overrides": {
    "history": "history.html"
  },
  "permissions": ["history", "favicon"],
  "icons": {
    "16": "history16.png",
    "32": "history32.png",
    "48": "history48.png",
    "128": "history128.png"
  }
}
