{
  "name": "Downloads Manager",
  "version": "0.3",
  "manifest_version": 3,
  "description": "Downloads Manager",
  "icons": {
    "128": "icon128.png"
  },
  "action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "Downloads Manager",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "default_locale": "en",
  "optional_permissions": [
    "management"
  ],
  "permissions": [
    "downloads",
    "downloads.open",
    "downloads.shelf",
    "storage"
  ]
}