{
  "name": "no-cookies Rule Manager",
  "version": "0.1",
  "manifest_version": 3,
  "description": "Manage in which conditions cookies are denied to be send",
  "background": {
    "service_worker": "service_worker.js"
  },
  "action": {
    "default_title": "no-cookies Rule Manager",
    "default_popup": "popup.html"
  },
  "host_permissions": ["<all_urls>"],
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "activeTab"
  ]
}
