{
  "manifest_version": 2,
  "name": "Multi-tabbed Browser",
  "minimum_chrome_version": "35.0.0.0",
  "version": "0.8",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["config.js", "main.js"]
    }
  },
  "permissions": [
    "contextMenus",
    "webview"
  ]
}
