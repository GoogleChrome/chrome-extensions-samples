{
  "name": "Servo Serial Sample",
  "version": "0.4",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "description": "Show off serial functionality.",

  "app": {
    "background": {
      "scripts": [ "background.js" ]
    }
  },

  "icons": {
    "16": "assets/icon-16x16.jpeg",
    "128": "assets/icon-128x128.jpeg"
  },

  "permissions": [
    "serial",
    "videoCapture"
  ]
}
