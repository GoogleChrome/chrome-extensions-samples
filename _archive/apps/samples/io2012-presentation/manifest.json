{
  "manifest_version": 2,
  "name": "Apps I/O 2012 Presentation",
  "description": "Chrome Apps IO 2012 presentation.",
  "version": "1.3",
  "minimum_chrome_version": "23",
  "app": {
    "background": {
      "scripts": ["js/main.js"]
    }
  },
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "webview",
    "serial",
    "storage",
    "videoCapture"
  ]
}
