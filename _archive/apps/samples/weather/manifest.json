{
  "name": "Weather",
  "description": "A simple weather application.",
  "manifest_version": 2,
  "version": "1.0.7",
  "minimum_chrome_version": "23",
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "icons": {
    "16": "img/icon.png",
    "64": "img/icon.png",
    "128": "img/icon.png"
  },
  "permissions": [
    "geolocation",
    "storage",
    "<all_urls>"
  ]
}
