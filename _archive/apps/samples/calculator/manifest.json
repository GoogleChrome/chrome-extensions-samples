{
  "name": "Calculator",
  "description": "A simple calculator.",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "version": "1.2",
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions" : [
    "clipboardWrite"
  ],
  "icons": {
    "16": "assets/icon-128x128.png",
    "128": "assets/icon-128x128.png"
  }
}
