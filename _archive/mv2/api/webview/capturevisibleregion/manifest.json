{
  "manifest_version": 2,
  "name": "Webview transparency",
  "description": "Sample of the webview.captureVisibleRegion api",
  "version": "1",
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
    "webview"
  ],
  "webview": {
    "partitions": [
      {
        "name": "partition",
        "accessible_resources": [ "test2.html" ]
      }
    ]
  }
}
