{
  "name": "Print Extension",
  "version": "1.0",
  "description": "Sends print job directly to the printers installed on the Chromebook",
  "permissions": [
    "printing"
  ],
  "browser_action": {
    "default_popup": "printers.html",
    "default_icon": "icons/icon.png"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 2
}
