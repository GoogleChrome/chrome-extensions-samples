{
  "name": "Sandboxed Frame",
  "description": "Demonstrate use of handlebars inside a sandboxed frame",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["notifications"],
  "background": {
    "page": "eventpage.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon" : "icon.png",
    "default_title": "Start Event Page"
  },
  "sandbox": {
    "pages": ["sandbox.html"]
  },
  "web_accessible_resources": ["icon.png"]
}
