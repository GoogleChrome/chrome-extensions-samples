{
  "manifest_version": 3,
  "name": "Calendar Mate",
  "version": "1.0",
  "description": "Create Google Calendar events from selected text using Gemini Nano.",
  "permissions": ["contextMenus"],
  "minimum_chrome_version": "138",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    }
  }
}
