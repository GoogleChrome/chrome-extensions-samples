{
  "name": "Anchor Element Clicker",
  "version": "1.0",
  "manifest_version": 3,
  "description": "A sample of clicking on an anchor element with javascript: scheme",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "file:///*/sample.inline-script-anchor-clicker/inline-script-demo.html"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "file:///*/sample.inline-script-anchor-clicker/inline-script-demo.html"
  ]
}