{
  "name": "Sharing Cookies via CORS",
  "description": "This extension demonstrates how a extension and coordinating website can share cookies.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "wrapper.js"
  },
  "action": { },
  "permissions": [
    "cookies"
  ],
  "host_permissions": [
    "*://crx-sharing-cookies.glitch.me/*"
  ],
  "key": ""
}
