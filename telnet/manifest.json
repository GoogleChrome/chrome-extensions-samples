{
  "name": "Telnet Client",
  "version": "1",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "icons": {
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["launch.js"],
      "transient": true
    }
  },
  "permissions": [
    "experimental",
    {"socket": [
      "tcp-connect:*:*"
    ]}
  ]
}
